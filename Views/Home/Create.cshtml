@{
    ViewData["Title"] = "Create";
}

@model WebApplication1.Models.Contacts

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<h2>Добавления пользователя</h2>
<form asp-action="create" asp-controller="home">
    <p>
        <label app-for="Name">Номер</label><br />
        <input type="number" asp-for="Number" />
    </p>
    <p>
        <label app-for="Name">Имя</label><br />
        <input type="text" asp-for="Name" />
    </p>
    <p>
        <label app-for="Age">Возраст</label><br />
        <input type="number" asp-for="Age" />
    </p>
    <p>
        <label app-for="Mail">Mail</label><br />
        <input type="text" asp-for="Mail" />
    </p>
    <p>
        <label app-for="Addres">Адрес</label><br />
        <input type="text" asp-for="Addres" />
    </p>
    <p>
        <input type="submit" value="Отправить" />
    </p>
</form>